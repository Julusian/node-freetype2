{
  "name": "freetype2",
  "version": "1.0.4",
  "description": "FreeType bindings for Node.js.",
  "main": "./index.js",
  "types": "./module.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericfreese/node-freetype2"
  },
  "keywords": [
    "freetype",
    "font",
    "webfont",
    "parse",
    "sfnt",
    "ttf",
    "otf",
    "woff",
    "truetype",
    "opentype"
  ],
  "author": "Julian Waller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfreese/node-freetype2/issues"
  },
  "homepage": "https://github.com/ericfreese/node-freetype2",
  "scripts": {
    "install": "node-gyp-build",
    "vendor": "node vendor/download-freetype.js",
    "prebuild": "prebuildify -r napi -t 12.0.0 -t electron@6.0.0 --strip",
    "prebuild-ia32": "prebuildify -r napi --arch=ia32 -t 12.0.0 -t electron@6.0.0 --strip",
    "clean": "node-gyp clean",
    "build": "node-gyp build",
    "test": "jest",
    "dev": "watch 'printf \"\\033[2J\\033[0f\" && yarn build && yarn test --watch' src spec"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.2",
    "node-gyp-build": "^4.2.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "ajv": "^6.12.6",
    "decompress": "^4.2.1",
    "decompress-tarxz": "^3.0.0",
    "download": "^8.0.0",
    "fs-extra-plus": "^0.5.20",
    "jest": "^26.6.3",
    "prebuildify": "^4.1.1",
    "sharp": "^0.26.3",
    "ts-json-schema-generator": "^0.79.0",
    "typescript": "^4.1.2"
  }
}
