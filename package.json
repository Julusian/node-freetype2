{
  "name": "@julusian/freetype2",
  "version": "0.5.0-0",
  "description": "FreeType bindings for Node.js.",
  "main": "./index.js",
  "types": "./module.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/julusian/node-freetype2"
  },
  "keywords": [
    "freetype",
    "font",
    "webfont",
    "parse",
    "sfnt",
    "ttf",
    "otf",
    "woff",
    "truetype",
    "opentype"
  ],
  "author": "Julian Waller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/julusian/node-freetype2/issues"
  },
  "homepage": "https://github.com/julusian/node-freetype2",
  "scripts": {
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "clean": "node-gyp clean",
    "build": "node-gyp build",
    "test": "yarn gen-schema && jest",
    "dev": "watch 'printf \"\\033[2J\\033[0f\" && yarn build && yarn test --watch' src spec",
    "gen-schema": "yarn typescript-json-schema module.d.ts \"*\" --strictNullChecks --required --noExtraProps -o ./spec/schema.json"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^2.0.0",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^13.13.0",
    "ajv": "^6.12.1",
    "jest": "^25.3.0",
    "prebuild": "^10.0.0",
    "sharp": "^0.25.2",
    "typescript-json-schema": "^0.42.0"
  }
}
