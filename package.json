{
  "name": "freetype2",
  "version": "1.0.1",
  "description": "FreeType bindings for Node.js.",
  "main": "./index.js",
  "types": "./module.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericfreese/node-freetype2"
  },
  "keywords": [
    "freetype",
    "font",
    "webfont",
    "parse",
    "sfnt",
    "ttf",
    "otf",
    "woff",
    "truetype",
    "opentype"
  ],
  "author": "Julian Waller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericfreese/node-freetype2/issues"
  },
  "homepage": "https://github.com/ericfreese/node-freetype2",
  "scripts": {
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "prebuild": "prebuild -r napi",
    "clean": "node-gyp clean",
    "build": "node-gyp build",
    "test": "jest",
    "dev": "watch 'printf \"\\033[2J\\033[0f\" && yarn build && yarn test --watch' src spec"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.2",
    "prebuild-install": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "ajv": "^6.12.6",
    "jest": "^26.6.3",
    "prebuild": "^10.0.1",
    "sharp": "^0.26.3",
    "ts-json-schema-generator": "^0.79.0",
    "typescript": "^4.1.2"
  }
}
